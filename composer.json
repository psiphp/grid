{
    "name": "psiphp/grid",
    "description": "Psi Grid component",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "psiphp/view": "^1.0@dev",
        "jms/metadata": "^1.5",
        "doctrine/annotations": "^1.2",
        "psiphp/object-agent": "^1.0@dev",
        "symfony/property-access": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "doctrine/collections": "^1.3"
    },
    "autoload": {
